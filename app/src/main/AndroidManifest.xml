<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bankingapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bank_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BankingApp">
        <activity android:name=".Detail" />
        <activity android:name=".fetch_data" />
        <activity android:name=".Home" />
        <activity android:name=".TransactionHistory" />
        <activity android:name=".RecieverList" />
        <activity android:name=".TransactionSuccessFull" />
        <activity android:name=".SuccessFullSplash" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>